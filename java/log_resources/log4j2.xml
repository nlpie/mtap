<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="MTAP Integration Logger">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="edu.umn.nlpie.mtap" level="DEBUG">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="INFO"/>
    </Loggers>
</Configuration>
